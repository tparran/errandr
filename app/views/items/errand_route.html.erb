<div class="page-header">
  <h1>
    Stores
  </h1>
</div>

<table class="table table-striped table-hover">
  <tr>
    <!-- <th>Image</th> -->
    <th>Item</th>
    <th>Store</th>
    <th>Address</th>
  </tr>

  <% @items.each do |item| %>
  <tr>
    <td><%= item.name %></td>
    <td><%= @closest_stores[@items.index(item)]%></td>
    <td><%= @closest_stores_addresses[@items.index(item)]%></td>
  </tr>
  <% end %>
</table>

<div class="page-header">
  <h1>
    Optimal Route
  </h1>
</div>

<table class="table table-striped table-hover">
  <tr>
    <!-- <th>Image</th> -->
    <th>Leg</th>
    <th>Destination</th>
  </tr>

  <% @optimized_waypoint_destination_addresses.each do |leg| %>
  <tr>
    <td><%= @optimized_waypoint_destination_addresses.index(leg)+1 %></td>
    <td><%= leg %></td>
  </tr>
  <% end %>
</table>

